[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group user/shortcuter4
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/shortcuter4
	label-Verified = -1..+1 group user/shortcuter4
	push = group user/shortcuter4
	read = group user/shortcuter4
	rebase = group user/shortcuter4
	removeReviewer = group user/shortcuter4
	submit = group user/shortcuter4
[access "refs/heads/*"]
	create = group user/shortcuter4
	push = +force group user/shortcuter4
	pushMerge = group user/shortcuter4
	read = group user/shortcuter4
[access "refs/tags/*"]
	push = group user/shortcuter4
	push = +force group user/shortcuter4
